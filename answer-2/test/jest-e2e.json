{
	"moduleFileExtensions": ["js", "json"],
	"rootDir": ".",
	"testRegex": ".e2e-spec.js$",
	"clearMocks": true,
	"coverageDirectory": "coverage",
	"collectCoverageFrom": ["**/*.js"],
	"coverageThreshold": {
		"global": {
			"branches": 100,
			"functions": 100,
			"lines": 100,
			"statements": 100
		}
	},
	"testEnvironment": "node"
}
